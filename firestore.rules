rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    
    // Chat Messages:
    // ALLOW EVERYTHING DEBUG MODE
    match /chat_messages/{messageId} {
      allow read, write: if true;
    }
    
    // Default: Deny other access
    match /{document=**} {
      allow read, write: if false;
    }
  }
}
